<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Patrocinador.
    -->
    <changeSet id="20180620020752-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="solicitud_id"
                                 baseTableName="patrocinador"
                                 constraintName="fk_patrocinador_solicitud_id"
                                 referencedColumnNames="id"
                                 referencedTableName="solicitud_patrocinio"/>

        <addForeignKeyConstraint baseColumnNames="patrocinadors_id"
                                 baseTableName="patrocinador_eventos"
                                 constraintName="fk_patrocinador_eventos_patrocinadors_id"
                                 referencedColumnNames="id"
                                 referencedTableName="patrocinador"/>
        <addForeignKeyConstraint baseColumnNames="eventos_id"
                                 baseTableName="patrocinador_eventos"
                                 constraintName="fk_patrocinador_eventos_eventos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="evento"/>
        
    </changeSet>
</databaseChangeLog>
